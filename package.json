    "eslint-plugin-jquery": "^1.5.1",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.5.0",
    "jsdom": "^26.1.0",
    "mini-css-extract-plugin": "^2.7.5",
    "prettier": "^3.3.2",
    "sass": "^1.91.0",
    "sass-loader": "^16.0.5",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "webpack": "^5.99.9",
    "webpack-bundle-tracker": "^3.1.1",
    "webpack-cli": "^6.0.1"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build_dev": "webpack --mode=development --config webpack.config.js --devtool source-map",
    "webpack_watch": "webpack --mode=development --config webpack.config.js --watch --devtool source-map",
    "lint": "eslint",
    "format": "eslint --fix",
    "cspell": "cspell"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "google",
      "plugin:security/recommended",
      "plugin:jquery/deprecated"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaVersion": 2020
    },
    "plugins": [
      "security",
      "jquery"
    ],
    "rules": {
      "quotes": [
        "error",
        "double",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ],
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline"
        }
      ],
      "complexity": [
        "error",
        {
          "max": 6
        }
      ],
      "indent": [
        "error",
        2,
        {
          "MemberExpression": 1
        }
      ],
      "max-len": [
        "error",
        {
          "code": 90,
          "tabWidth": 4
        }
      ],
      "no-use-before-define": [
        "error",
        {
          "functions": true,
          "classes": true
        }
      ],
      "object-curly-spacing": [
        "error",
        "never"
      ],
      "object-shorthand": [
        "error",
        "always"
      ],
      "wrap-iife": [
        "error",
        "outside"
      ]
    },
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "parser": "@typescript-eslint/parser",
        "plugins": [
          "@typescript-eslint"
        ],
        "rules": {
          "no-unused-vars": "off",
          "@typescript-eslint/no-unused-vars": [
            "error",
            {
              "argsIgnorePattern": "^_",
              "varsIgnorePattern": "^_",
              "caughtErrorsIgnorePattern": "^_"
            }
          ]
        }
      }
    ]
  }
}
